{
  "name": "authok",
  "version": "0.0.2",
  "description": "",
  "author": "",
  "private": true,
  "type": "commonjs",
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf dist",
    "format": "prettier --write \"apps/**/*.ts\" \"libs/**/*.ts\"",
    "cli": "CLI_PATH=./apps/cli/src/main.ts yarn nestjs-command",
    "build:cli": "nest build cli",
    "build": "nest build api-server",
    "build:api-server": "nest build api-server",
    "start:api-server": "nest start api-server",
    "start:api-server:dev": "nest start --watch api-server",
    "start:api-server:debug": "nest start --debug --watch api-server",
    "start:api-server:prod": "node dist/apps/api-server/main",
    "build:mgmt-api-server": "nest build mgmt-api-server",
    "start:mgmt-api-server": "nest start mgmt-api-server",
    "start:mgmt-api-server:dev": "nest start --watch mgmt-api-server",
    "start:mgmt-api-server:debug": "nest start --debug --watch mgmt-api-server",
    "start:mgmt-api-server:prod": "node dist/apps/mgmt-api-server/main",
    "build:tenant-service": "nest build tenant-service",
    "start:tenant-service": "nest start api-server",
    "start:tenant-service:dev": "nest start --watch tenant-service",
    "start:tenant-service:debug": "nest start --debug --watch tenant-service",
    "start:tenant-service:prod": "node dist/apps/tenant-service/main",
    "start:task-center:dev": "nest start --watch task-center",
    "start:tunnel": "ngrok start -config ngrok.yml api",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:api-server:e2e": "jest --config ./apps/api-server/test/jest-e2e.json",
    "changelog": "conventional-changelog -i CHANGELOG.md -s -r 0 && git add CHANGELOG.md",
    "db.tenant:migrate": "ts-node -r tsconfig-paths/register --transpile-only ./node_modules/typeorm/cli.js -d ./libs/support/tenant-support-typeorm/src/ormconfig.tenant.ts migration:run",
    "db.tenant:generate": "ts-node -r tsconfig-paths/register --transpile-only ./node_modules/typeorm/cli.js -d ./libs/support/tenant-support-typeorm/src/ormconfig.tenant.ts migration:generate ./libs/support/tenant-support-typeorm/migrations/generated",
    "db.authok:migrate": "ts-node -r tsconfig-paths/register --transpile-only ./node_modules/typeorm/cli.js -d ./libs/support/infra-support-typeorm/src/ormconfig.authok.ts migration:run",
    "db.authok:generate": "ts-node -r tsconfig-paths/register --transpile-only ./node_modules/typeorm/cli.js -d ./libs/support/infra-support-typeorm/src/ormconfig.authok.ts migration:generate ./libs/support/infra-support-typeorm/migrations/generated",
    "generate:proto": "protoc --plugin=./node_modules/.bin/protoc-gen-ts_proto -I=./proto --ts_proto_opt=nestJs=true --ts_proto_out=./proto/stub --ts_proto_opt=nestJs=true --ts_proto_opt=fileSuffix=.pb ./proto/**/*.proto"
  },
  "dependencies": {
    "@authenio/samlify-xsd-schema-validator": "^1.0.3",
    "@authok/action-core-module": "./packages/action-core-module",
    "@authok/nestjs-openid-connect": "^1.0.1",
